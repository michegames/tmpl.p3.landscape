{
  "name": "template-parcel",
  "version": "1.0.0",
  "description": "A Phaser 3 template using Parcel 2 as the bundler.",
  "source": "./index.html",
  "browserslist": "> 0.25%, last 2 versions, not dead",
  "scripts": {
    "dev": "parcel index.html --no-cache --config ./parcel/.parcel-dev -p 9090 --https",
    "build": "parcel build index.html --no-source-maps --log-level error --config ./parcel/.parcel-prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michegames/template-parcel.git"
  },
  "author": "Phaser Studio <support@phaser.io> & Michelangelo <michelangelo@etik.com>",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "bugs": {
    "url": "https://github.com/phaserjs/template-parcel/issues"
  },
  "homepage": "https://github.com/michegames/template-parcel#readme",
  "devDependencies": {
    "@parcel/transformer-glsl": "^2.11.0",
    "express": "^4.18.2",
    "parcel": "^2.11.0",
    "parcel-reporter-clean-dist": "^1.0.4",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "process": "^0.11.10"
  },
  "dependencies": {
    "phaser": "^3.70.0"
  },
  "staticFiles": {
    "staticPath": "public"
  }
}
